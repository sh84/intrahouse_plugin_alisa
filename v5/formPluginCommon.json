{
  "grid": [
    { "id": "p1", "xs": 6, "class": "main", "table": "units" },
    { "id": "p2", "xs": 6, "class": "main", "table": "units" },
    { "id": "log_table", "xs": 12, "class": "main", "table": "unitsTreeTable", "height": "fill" }
  ],

  "spacing": 10,

  "p1": [{
    "prop": "port",
    "title": "Порт",
    "type": "input"
  }, {
    "prop": "ya_oauth_user_token",
    "title": "OAuth-токен пользователя",
    "type": "input"
  }, {
    "prop": "ya_oauth_client_secret",
    "title": "Oauth cекрет приложения",
    "type": "input"
  }, {
    "prop": "ya_skill_id",
    "title": "Идентификатор диалога",
    "type": "input"
  }],

  "p2": [{
    "prop": "restarttime",
    "title": "$Unit_Restarttime",
    "type": "number",
    "default": 5
  }, {
    "prop": "loglevel",
    "title": "$LogLevel",
    "type": "droplist",
    "data": "loglevelList"
  }],

  "log_table": [{
    "title": "",
    "type": "table",
    "prop": "unitsTreeTable",
    "columns": [
      { "prop": "name", "title": "ID", "type": "text", "width": 150 },
      { "prop": "state", "title": "Состояние", "type": "text", "width": 150 },
      { "prop": "laststart", "title": "Время запуска", "type": "text", "width": 150 },
      { "prop": "laststop", "title": "Время останова", "type": "text", "width": 150 },
      { "prop": "uptime", "title": "Uptime", "type": "text", "width": 150 },
      { "prop": "error", "title": "Ошибка", "type": "text", "width": 250 }
    ]
  }]
}